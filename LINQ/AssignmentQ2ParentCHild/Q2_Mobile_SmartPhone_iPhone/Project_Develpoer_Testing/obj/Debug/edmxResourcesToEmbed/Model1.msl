        </MappingFragment>
      </EntityTypeMapping>
      <EntityTypeMapping TypeName="IsTypeOf(richModel.Deployment)">
        <MappingFragment StoreEntitySet="Deployment">
          <ScalarProperty Name="FloorName" ColumnName="FloorName" />
